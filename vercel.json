{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/lod",
      "destination": "/lod.html"
    },
    {
      "source": "/thanos",
      "destination": "/thanos.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "cross-origin"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "cross-origin"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
